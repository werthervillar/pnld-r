                      "property": "Colaborador",
                      "sortable": true,
                      "title": "Colaborador",
                      "type": "string",
                      "visible": true
                    },
                    {
                      "property": "Responsavel",
                      "title": "Responsável",
                      "type": "string"
                    },
                    {
