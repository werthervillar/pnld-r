                              {
                                "name": "$filter",
                                "value": "ColaboradorEmail eq '${security.user.name}'"
                              }
                            ],
                            "then": [
                              {
                                "condition": "",
                                "name": "getReembolsosDespesasListsResult",
                                "type": "set",
                                "value": "${result.value}"
                              }
                            ],
                            "type": "invoke"
                          },
                          {
                            "condition": "${security.user.isInRole(\"Colaborador\")} != true",
                            "name": "pnld.getReembolsosDespesasLists",
                            "then": [
                              {
                                "name": "getReembolsosDespesasListsResult",
                                "type": "set",
                                "value": "${result.value}"
                              }
                            ],
                            "type": "invoke"
                          }
                        ],
