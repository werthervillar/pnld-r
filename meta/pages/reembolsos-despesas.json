                        ],
                        "then": [
                          {
                            "name": "pnld.getReembolsosDespesasLists",
                            "then": [
                              {
                                "name": "getReembolsosDespesasListsResult",
                                "type": "set",
                                "value": "${result.value}"
                              }
                            ],
                            "type": "invoke"
                          },
                          {
                            "detail": "ReembolsosDespesa deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
