          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Novo",
                  "allowAdd": "${security.user.isInRole(\"Colaborador\") == false}",
                  "allowDelete": "${security.user.isInRole(\"Colaborador\") == false}",
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
