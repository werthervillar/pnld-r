                      {
                        "name": "pnld.getReembolsosDespesas",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$expand",
                            "value": "ItensReembolsosDespesas"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "ItensReembolsosDespesas"
                          }
                        ],
                        "then": [
                          {
                            "name": "getReembolsosDespesasResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getReembolsosDespesasCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "condition": "",
                        "name": "pnld.getReembolsosDespesasLists",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          }
                        ],
                        "then": [
                          {
                            "name": "getReembolsosDespesasListsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getReembolsosDespesasListsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "ReembolsoDespesa",
                            "value": "${event.ReembolsoDespesa}"
