                    {
                      "filterable": false,
                      "sortable": false,
                      "template": [
                        {
                          "buttonStyle": "warning",
                          "events": {
                            "click": [
                              {
                                "parameters": [
                                  {
                                    "name": "ReembolsoDespesa",
                                    "value": "${data.ReembolsoDespesa}"
                                  }
                                ],
                                "path": "edit-pendencias",
                                "title": "Histórico e Pendências",
                                "type": "popup",
                                "width": 800
                              }
                            ]
                          },
                          "icon": "change_history",
                          "name": "button0",
                          "size": "sm",
                          "text": "Pendências",
                          "type": "button"
                        }
                      ],
                      "type": "string",
                      "width": "120px"
                    },
                    {
                      "filterable": false,
                      "footerTemplate": "",
